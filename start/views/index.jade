extends layout

block content

  div.mdl-tabs.mdl-js-tabs.mdl-js-ripple-effect
    div.mdl-tabs__tab-bar
      a(class='mdl-tabs__tab is-active', href='#tab1') Tab One
      a(class='mdl-tabs__tab', href='#tab2') Tab Two
      a(class='mdl-tabs__tab', href='#tab3') Tab Three

    div#tab1.mdl-tabs__panel.is-active
      p First tab's content.

    div#tab2.mdl-tabs__panel
      p Second tab's content.

    div#tab3.mdl-tabs__panel
      p Third tab's content.
